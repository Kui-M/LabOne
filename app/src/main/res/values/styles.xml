<resources>

    <!-- Base application theme. -->
    <style name="AppTheme" parent="Theme.AppCompat.Light.DarkActionBar">
        <!-- Customize your theme here. -->
        <item name="colorPrimary">@color/material_teal_600</item>
        <item name="colorPrimaryDark">@color/material_teal_800</item>
        <item name="colorAccent">@color/material_brown_200</item>
    </style>

    <style name="AppTheme.NoActionBar">
        <item name="windowActionBar">false</item>
        <item name="windowNoTitle">true</item>
    </style>

    <style name="AppTheme.AppBarOverlay" parent="ThemeOverlay.AppCompat.Dark.ActionBar" />

    <style name="AppTheme.PopupOverlay" parent="ThemeOverlay.AppCompat.Light" />

    <style name="EditText">
        <item name="android:bottomLeftRadius">5dp</item>
        <item name="android:bottomRightRadius">5dp</item>
        <item name="android:topRightRadius">5dp</item>
        <item name="android:topLeftRadius">5dp</item>
    </style>


</resources>
